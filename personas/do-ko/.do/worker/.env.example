# Database configuration
# Type: sqlite (default) or mysql
DO_DB_TYPE=sqlite

# SQLite configuration
DO_DB_PATH=.do/memory.db

# MySQL configuration (when DO_DB_TYPE=mysql)
# DO_DB_HOST=localhost
# DO_DB_PORT=3306
# DO_DB_USER=doworker
# DO_DB_PASSWORD=secret
# DO_DB_DATABASE=do_memory

# User configuration
DO_USER_NAME=max
